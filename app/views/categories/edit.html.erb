<h1> Edita la categoria  <%= @category.nombre %> </h1>
<p> <em> Ingrese datos necesarios: </em> </p>
<%= render partial: "/shared/errors", locals: {obj: @category} %>
<%= form_for @category do |f| %>
	<%= render partial: "form", locals: {f: f} %>
	<p> <%= f.submit  "Actualizar" %> </p>
  <p><%= link_to "Borrar", @category, method: :delete, data: {confirm: "Estas Seguro?"}  %></p>
	<p> <%= link_to "Cancelar", categories_path %> </p>

<% end%>
